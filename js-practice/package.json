{
  "name": "interns-projects",
  "version": "1.0.0",
  "description": "Goal of the project is to provide the platform to write algorithms and tests",
  "scripts": {
    "test": "jest",
    "lint": "eslint tasks/**/*.js",
    "lint:write": "npm run lint -- --fix",
    "tdd": "jest --watch",
    "prettier": "prettier tasks/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "**/*.js": "prettier --write --ignore-unknown"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "husky": "^4.3.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2"
  }
}
